<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mcdonalds"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="155dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="107dp"
        android:text="@string/mdFood1"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/qPounderMeal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="19dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:text="@string/mdFood2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="12dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mcNuggetsMeal"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        android:text="@string/mdFood4" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="52dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        android:text="@string/mdFood5" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="17dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/mcNuggetsMeal"
        android:text="@string/mdFood6" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/applePie"
        app:layout_constraintEnd_toStartOf="@+id/applePie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/applePie"
        android:text="@string/mdFood7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="41dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/mdFood3" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/imgdes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019"
        app:srcCompat="@drawable/mcdonalds" />

    <Button
        android:id="@+id/order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:text="@string/order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/bigMacMeal"
        android:layout_width="61dp"
        android:layout_height="40dp"
        android:layout_marginTop="126dp"
        android:layout_marginBottom="127dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/order"
        app:layout_constraintStart_toEndOf="@+id/order"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/mAngusOrMeal"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="54dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/qPounderMeal"
        app:layout_constraintEnd_toEndOf="@+id/bigMacMeal"
        app:layout_constraintStart_toStartOf="@+id/bigMacMeal"
        app:layout_constraintTop_toTopOf="@+id/bigMacMeal"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/qPounderMeal"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/mAngusOrMeal"
        app:layout_constraintStart_toStartOf="@+id/mAngusOrMeal"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/mcChickenMeal"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="104dp"
        android:layout_marginBottom="104dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/mcMuffinMeal"
        app:layout_constraintEnd_toEndOf="@+id/mcNuggetsMeal"
        app:layout_constraintStart_toStartOf="@+id/mcNuggetsMeal"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/mcNuggetsMeal"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="52dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/mcMuffinMeal"
        app:layout_constraintEnd_toEndOf="@+id/mcMuffinMeal"
        app:layout_constraintStart_toStartOf="@+id/mcMuffinMeal"
        app:layout_constraintTop_toTopOf="@+id/mcChickenMeal"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/mcMuffinMeal"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginBottom="79dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/qPounderMeal"
        app:layout_constraintStart_toStartOf="@+id/qPounderMeal"
        tools:ignore="Autofill"/>

    <EditText
        android:id="@+id/applePie"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:hint="@string/hint6"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/mcMuffinMeal"
        app:layout_constraintStart_toStartOf="@+id/mcMuffinMeal"
        app:layout_constraintTop_toBottomOf="@+id/mcMuffinMeal"
        tools:ignore="Autofill"/>

</android.support.constraint.ConstraintLayout>